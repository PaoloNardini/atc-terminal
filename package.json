  "scripts": {
    "start": "APP_VERSION=$npm_package_version node dist/start.js",
    "dev": "export $(grep -v '^#' .env | xargs) && APP_VERSION=$npm_package_version nodemon --exec ts-node --files --project tsconfig.json src/start.ts",
    "clean": "rm -rf dist",
    "build": "run-s clean && run-p build:* && run-s test",
    "build:server": "tsc",
    "build:assets": "webpack -c webpack.config.js",
    "test": "jest --passWithNoTests",
    "image:prod": "docker build . -f ./docker/Prod.dockerfile -t eu.gcr.io/webravo-175810/$npm_package_name:v$npm_package_version",
    "deploy:registry:push": "docker push eu.gcr.io/webravo-175810/$npm_package_name:v$npm_package_version"
  },
